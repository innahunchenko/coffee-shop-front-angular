{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "CacheSettings": {
    "DefaultCacheDurationMinutes": 60,
    "RedisConnectionString": "redis:6379"
  }
}
